
allow group compute_admin to manage cluster-family in compartment appgroup1:systems where request.permission!=/*_DELETE/ 
allow group compute_admin to manage cluster-family in compartment systems where request.permission!=/*_DELETE/ 
allow group compute_admin to manage instance-family in compartment appgroup1:systems where request.permission!=/*_DELETE/ 
allow group compute_admin to manage instance-family in compartment systems where request.permission!=/*_DELETE/ 
allow group compute_admin to manage tag-namespaces in tenancy where all { any { request.permission='TAG_NAMESPACE_INSPECT', request.permission='TAG_NAMESPACE_READ' }, target.tag-namespace.name='tag_cost-tracking' } 
allow group compute_admin to manage tag-namespaces in tenancy where all { any { request.permission='TAG_NAMESPACE_INSPECT', request.permission='TAG_NAMESPACE_READ' }, target.tag-namespace.name='tag_resource initial' } 
allow group compute_admin to manage volume-family in compartment appgroup1:storage where all { any { request.permission='VOLUME_BACKUP_INSPECT' } } 
allow group compute_admin to manage volume-family in compartment appgroup1:storage where all { any { request.permission='VOLUME_INSPECT', request.permission='VOLUME_UPDATE', request.permission='VOLUME_WRITE' } } 
allow group compute_admin to manage volume-family in compartment storage where all { any { request.permission='VOLUME_BACKUP_INSPECT' } } 
allow group compute_admin to manage volume-family in compartment storage where all { any { request.permission='VOLUME_INSPECT', request.permission='VOLUME_UPDATE', request.permission='VOLUME_WRITE' } } 
